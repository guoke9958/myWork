	<script language="javascript">
		function getdata() {
			var name = document.getElementById("user_name").value;
			
			if (name == "") {
				alert("名字不能为空");
				return false;
			}
			
			var phone = document.getElementById("user_phone").value;
			
			if (phone == "") {
				alert("手机号不能为空");
				return false;
			}
			
			var address = document.getElementById("user_address").value;
			
			if (address == "") {
				alert("联系地址不能为空");
				return false;
			}
			
			var sex = document.getElementsByName("select1")[0].value;
			var age = document.getElementsByName("select2")[0].value;
			var xueli = document.getElementsByName("select3")[0].value;
			var work = document.getElementsByName("select4")[0].value;
			var joinPrice = document.getElementsByName("select5")[0].value;
			var joinHui = document.getElementsByName("select6")[0].value;
			var peixun = document.getElementsByName("select7")[0].value;
			var introduce = document.getElementsByName("select8")[0].value;
			var pingjia = document.getElementById("pingjia").value;

			window.location.href = "http://www.qiuyiwang.com:8081/Doctor/api/joinour/submit?name="
					+ encodeURI(encodeURI(name))
					+ "&phone="
					+ encodeURI(encodeURI(phone))
					+ "&address="
					+ encodeURI(encodeURI(address))
					+ "&sex="
					+ encodeURI(encodeURI(sex))
					+ "&age="
					+ encodeURI(encodeURI(age))
					+ "&xueli="
					+ encodeURI(encodeURI(xueli))
					+ "&work="
					+ encodeURI(encodeURI(work))
					+ "&joinPrice="
					+ encodeURI(encodeURI(joinPrice))
					+ "&joinHui="
					+ encodeURI(encodeURI(joinHui))
					+ "&peixun="
					+ encodeURI(encodeURI(peixun))
					+ "&introduce="
					+ encodeURI(encodeURI(introduce))
					+"&pingjia="
					+ encodeURI(encodeURI(pingjia));

			return true;
		}
	</script>

