<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:color="#ffeb6055" android:shape="rectangle" android:state_pressed="false">
            <corners android:radius="60.0dip" />
            <stroke android:width="0.0dip" android:color="#ffeb6055" />
            <solid android:color="#ffeb6055" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:color="@color/normal_red_press" android:shape="rectangle" android:state_pressed="false">
            <corners android:radius="60.0dip" />
            <stroke android:width="0.0dip" android:color="@color/normal_red_press" />
            <solid android:color="@color/normal_red_press" />
        </shape>
    </item>
</selector>